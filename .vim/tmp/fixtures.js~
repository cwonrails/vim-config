export const testUploadProfilePic = (blob, cb) => {
  setTimeout(() => {
    console.log('blob:');
    console.log(blob);

    // good
    cb(null, 'https://pixabay.com/static/uploads/photo/2015/03/04/22/35/head-659652_960_720.png');

    // bad
    // cb(new Error());
  }, 2000);
};
